<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<RootNamespace>Mobile</RootNamespace>
		<ProduceReferenceAssembly>true</ProduceReferenceAssembly>
		<LangVersion>8.0</LangVersion>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)'=='Debug'">
		<DebugType>portable</DebugType>
		<DebugSymbols>true</DebugSymbols>
	</PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="Resources\Fonts\fa-brands-400.ttf" />
		<EmbeddedResource Include="Resources\Fonts\fa-regular-400.ttf" />
		<EmbeddedResource Include="Resources\Fonts\fa-solid-900.ttf" />
		<EmbeddedResource Include="settings.json" Condition="'$(Configuration)' != 'Debug' or !Exists('local.settings.json')" />
		<EmbeddedResource Include="local.settings.json" Link="settings.json" Condition="'$(Configuration)' == 'Debug' and Exists('local.settings.json')" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Xamarin.Essentials" Version="1.5.3.2" />
		<ProjectReference Include="..\..\XamarinComponents\XPlat\ExposureNotification\source\Xamarin.ExposureNotification\Xamarin.ExposureNotification.csproj">
		  <Project>{c264c0f9-3181-406e-992a-39a0302c9dd2}</Project>
		  <Name>Xamarin.ExposureNotification</Name>
		</ProjectReference>
		<PackageReference Include="Xamarin.Forms" Version="4.6.0.847" />
		<PackageReference Include="Xamarin.Forms.Visual.Material" Version="4.6.0.847" />
		<PackageReference Include="Acr.UserDialogs" Version="7.1.0.442" />
		<PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
		<PackageReference Include="Plugin.LocalNotification" Version="5.0.4" />
		<PackageReference Include="Refractored.MvvmHelpers" Version="1.6.2" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Shared\Shared.csproj" />
	</ItemGroup>

</Project>
